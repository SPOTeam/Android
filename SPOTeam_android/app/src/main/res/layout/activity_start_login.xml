<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".login.StartLoginActivity">



    <ImageView
        android:id="@+id/activity_start_login_logo_iv"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:src="@drawable/spot_homelogo"
        android:layout_marginTop="170dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/startstudy_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="7dp"
        android:text="@string/studystart"
        android:textSize="19.8dp"
        android:textColor="@color/b500"
        android:fontFamily="@font/suit_bold"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_start_login_logo_iv"/>


    <android.widget.Button
        android:id="@+id/item_logo_kakao_ib"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:drawableStart="@drawable/kakao_logo"
        android:paddingStart="10dp"
        android:paddingEnd="20dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="카카오톡으로 3초 시작하기"
        android:textColor="#3C1E1E"
        android:textSize="14dp"
        android:textFontWeight="600"
        android:background="@drawable/kakao_bt"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@id/item_logo_naver_ib"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <android.widget.Button
        android:id="@+id/item_logo_naver_ib"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:drawableStart="@drawable/naver_logo"
        android:paddingStart="10dp"
        android:paddingEnd="20dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="네이버로 시작하기"
        android:textFontWeight="600"
        android:textColor="#FFFFFF"
        android:textSize="14dp"
        android:background="@drawable/naver_bt"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>







</androidx.constraintlayout.widget.ConstraintLayout>