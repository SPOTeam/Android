            android:id="@+id/privacy_first_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/privacy_first_title"
            android:fontFamily="@font/suit_variable_font"
            android:textFontWeight="600"
            android:textSize="15dp"
            android:textColor="@color/black"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@id/privacy_title_tv"
            app:layout_constraintTop_toBottomOf="@id/privacy_title_tv"/>

        <TextView
            android:id="@+id/privacy_first_content_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/privacy_first_content"
            android:fontFamily="@font/suit_variable_font"
            android:textFontWeight="500"
            android:textSize="14dp"
            android:textColor="@color/black"
            android:layout_marginTop="5dp"
            android:layout_marginStart="9dp"
            app:layout_constraintStart_toStartOf="@id/privacy_first_title_tv"
            app:layout_constraintTop_toBottomOf="@id/privacy_first_title_tv"
            android:lineSpacingExtra="4dp"/>

        <!--    2. 회원 기본 원칙    -->
